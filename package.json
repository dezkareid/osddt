{
  "name": "@dezkareid/osddt",
  "version": "0.0.0",
  "description": "Package for Spec-Driven Development workflow",
  "keywords": [
    "setup",
    "package",
    "npm",
    "spec-driven",
    "cli"
  ],
  "type": "module",
  "bin": {
    "osddt": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "commander": "12.0.0",
    "fs-extra": "11.2.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/fs-extra": "11.0.4",
    "@types/node": "25.0.10",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "rollup": "4.56.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
